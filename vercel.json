{
    "version": 2,
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
     { "src": "game.js", "use": "@vercel/static" },
     { "src": "styles.css", "use": "@vercel/static" },
      { "src": "blue.mp3", "use": "@vercel/static" },
      { "src": "red.mp3", "use": "@vercel/static" },
      { "src": "yellow.mp3", "use": "@vercel/static" },
      { "src": "game.png", "use": "@vercel/static" },
      { "src": "green.mp3", "use": "@vercel/static" },
      { "src": "wrong.mp3", "use": "@vercel/static" },
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
